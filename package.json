{
  "name": "remote",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "node -r ts-node/register ./server.ts",
    "watch": "nodemon",
    "start": "node ./dist/bundle.js",
    "build:ts": "tsc",
    "build:webpack": "webpack",
    "build": "webpack",
    "serve": "node ./client/serve.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "robotjs": "^0.6.0",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.1",
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.1",
    "nodemon": "^2.0.13",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      ".",
      "./*",
      "**"
    ],
    "exec": "npm run dev",
    "ext": "ts",
    "delay": 1500
  },
  "pkg": {
    "targets": [
      "node14-win-x64"
    ]
  },
  "bin": "./dist/bundle.js"
}
